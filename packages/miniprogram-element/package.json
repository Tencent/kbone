{
	"name": "miniprogram-element",
	"version": "2.2.13",
	"description": "web's adapter for miniprogram",
	"main": "src/index.js",
	"miniprogram": "dist",
	"scripts": {
		"build": "rimraf dist && node tool/index.js",
		"test": "jest --bail",
		"test-debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --bail",
		"coverage": "jest --coverage --bail"
	},
	"jest": {
		"testEnvironment": "jsdom",
		"testURL": "https://jest.test",
		"testMatch": [
			"**/test/*.test.js"
		],
		"collectCoverageFrom": [
			"src/**/*.js"
		],
		"moduleDirectories": [
			"node_modules",
			"src"
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/wechat-miniprogram/kbone.git"
	},
	"author": "wechat-miniprogram",
	"license": "MIT",
	"peerDependencies": {
		"miniprogram-render": "*"
	},
	"devDependencies": {
		"copy-webpack-plugin": "^6.0.2",
		"jest": "^26.0.1",
		"miniprogram-simulate": "^1.2.6",
		"rimraf": "^3.0.2",
		"terser-webpack-plugin": "^3.0.6",
		"webpack": "^4.43.0"
	},
	"gitHead": "335714e2b6cd9c12d159dfc0753c697e800974a4"
}
